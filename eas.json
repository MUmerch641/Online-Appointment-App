{
  "cli": {
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "android": {
        "buildType": "apk"
      },
      "env": {
        "ENV": "development"
      }
    },
    "testing": {
      "android": {
        "buildType": "apk"
      },
      "env": {
        "ENV": "testing"
      }
    },
    "apkRelease": {
      "android": {
        "buildType": "apk"
      },
      "env": {
        "ENV": "production"
      }
    },
    "production": {
      "android": {
        "buildType": "app-bundle"
      },
      "env": {
        "ENV": "production"
      }
    }
  }
}
